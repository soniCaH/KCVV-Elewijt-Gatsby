{
  "eslint.enable": true,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  // prettier extension setting
  "editor.formatOnSave": true,
  "prettier.configPath": ".prettierrc.js",
  "cSpell.enableFiletypes": ["!javascript"]
}
