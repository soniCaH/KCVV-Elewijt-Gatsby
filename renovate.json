{
  "extends": [
    "config:base",
    "config:js-app",
    "packages:gatsby",
    "group:gatsbyMonorepo"
  ],
  "ignorePaths": [
    "_____BU-src",
    "_____BU-src/plugins/social-media-feed/package.json"
  ],
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "depTypeList": ["devDependencies"],
      "automerge": true
    }
  ],
  "labels": [
    "dependencies"
  ],
  "masterIssue": true
}
