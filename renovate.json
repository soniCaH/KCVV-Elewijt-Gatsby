{
  "extends": [
    "config:base",
    "config:js-app",
    "packages:gatsby",
    "group:gatsbyMonorepo",
    "schedule:weekly",
    "schedule:weekends"
  ],
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": false
    },
    {
      "depTypeList": ["devDependencies"],
      "automerge": false
    }
  ],
  "labels": [
    "dependencies"
  ],
  "prConcurrentLimit": 1,
  "timezone": "Europe/Brussels",
  "masterIssue": true
}
