{
  "extends": [
    "config:recommended",
    "config:js-app",
    "packages:gatsby",
    "group:gatsbyMonorepo"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    }
  ],
  "labels": [
    "dependencies"
  ],
  "timezone": "Europe/Brussels",
  "dependencyDashboard": true
}
